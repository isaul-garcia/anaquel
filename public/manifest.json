{
  "manifest_version": 3,
  "name": "Anaquel - Bookmarks Explorer",
  "author": "Isaul Garcia",
  "version": "1.0.0",
  "description": "Do bookmarks better",
  "icons": {
    "16": "icon_16.ico",
    "48": "icon_48.ico",
    "128": "icon_128.ico"
  },
  "browser_action": {
      "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks", "activeTab"]
}